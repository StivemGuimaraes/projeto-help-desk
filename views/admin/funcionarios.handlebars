<head>
  <title>Listas de Funcionarios</title>
</head>
<header
  class="py-3"
  style="
        background: rgb(45, 111, 131);
        background: linear-gradient(
          90deg,
          rgba(45, 111, 131, 1) 17%,
          rgba(28, 46, 76, 1) 100%
        );
      "
>
  <div class="container-fluid">
    <div class="row align-items-center">
      <div class="col-md-2 col-sm-1 col-xl-1">
        <a href="/client/index.html" class="text-decoration-none text-white">
          <img class="img-fluid float-start" src="" alt="logo aqui" />
        </a>
      </div>
      <div class="col-md-10 col-sm-11 col-xl-11">
        <a href="index.html" class="text-white text-decoration-none">
          <h1 class="text-start fs-2">academy help desk</h1>
        </a>
      </div>
    </div>
  </div>
</header>
<main>
  {{> _msg}}
  {{#each funcionario}}
  <div class="container w-50">
  <div class="card mt-4 mb-4">
  <h5 class="card-header">{{this.matricula}}</h5>
  <div class="card-body">
    <h5 class="card-title">{{this.usuario}}</h5>
    <p class="card-text">Senha: {{this.senha}}</p>
    <a href="#" class="btn btn-warning">Alterar</a>
    <a href="#" class="btn btn-danger">Deletar</a>
  </div>
</div>
</div>
  {{/each}}
</main>